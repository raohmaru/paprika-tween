{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": "./src/"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
      "template": "./node_modules/minami",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "readme": "README.md",
      "tutorials": "./tutorials/"
  }
}